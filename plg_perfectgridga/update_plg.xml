<?xml version="1.0" encoding="utf-8"?>
<updates>
  <update>
    <name>{{name}}</name>
    <version>{{version}}</version>
    <infourl title="{{name}}"><![CDATA[https://lightweight-ga.netlify.com]]></infourl>
    <downloads>
      <downloadurl type="full" format="zip"><![CDATA[https://lightweight-ga.netlify.com/pkgs/{{filename}}]]></downloadurl>
    </downloads>
    <targetplatform name="joomla" version="3.*"/>
    <element>{{codeName}}</element>
    <type>{{type}}</type>
    <folder>{{folder}}</folder>
    <client>{{client}}</client>
  </update>
</updates>
